{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "app",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/app.js",
      "env": {}
    },
    {
      "type": "node",
      "request": "launch",
      "name": "appPG",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/app.js",
      "env": {
        "DATABASE": "postgres://postgres:postgres@localhost:5432/test-tui.editor"
      }
    }
  ]
}
